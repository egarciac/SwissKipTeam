@model List<SwissKip.Web.Models.BankAccountModel>
@{
    ViewBag.Title = "Create";
}

<div id="contenido">
  	<div style="float:left; padding-left:100px;">
    	<div class="rojo25" style="float:left; z-index:0;">
        	<div style="float:left; padding-bottom:2px;">My stuff</div>
        	<div style="float:left; padding-left:235px;"><a href="/FileManager/Index" onMouseDown="MM_swapImage('menu11','','/Content/images/documentos.png',1)" onMouseOver="MM_swapImage('menu11','','/Content/images/documentos-on.png',0)" onMouseOut="MM_swapImgRestore()"><img src="/Content/images/documentos.png" border="0" name="menu11"></a></div>
            <div style="float:left; padding-left:8px;"><img src="/Content/images/cuentas-bancarias.png"></div>
            <div style="float:left; padding-left:8px;"><a href="/DigitalAccount/Index" onMouseDown="MM_swapImage('menu12','','/Content/images/cuentas-digitales.png',1)" onMouseOver="MM_swapImage('menu12','','/Content/images/cuentas-digitales-on.png',0)" onMouseOut="MM_swapImgRestore()"><img src="/Content/images/cuentas-digitales.png" border="0" name="menu12"></a></div>
        </div>
    	<div style="float:left; width:738px; height:1px; z-index:1; margin-top:-7px; background:#b5b5b3;"></div>
        <div style="float:left; width:830px; padding-top:25px;">
            <div style="float:left; padding-top:40px; padding-left:35px;">
               <div style="float:left; width:700px;">
                    <div style="float:left; width:70px;">
                        <div style="float:left; width:70px;" class="textonegrita15">Banco</div>
                    </div>
                    <div style="float:left; width:170px; padding-left:60px;">
                        <div style="float:left; width:170px; padding-left:10px;" class="textonegrita15">Número de cuenta</div>
                    </div>
                    <div style="float:left; width:70px; padding-left:40px;">
                        <div style="float:left; width:70px;" class="textonegrita15">Clave</div>
                    </div>
                    <div style="float:left; width:70px; padding-left:40px;">
                        <div style="float:left; width:70px; padding-left:12px;" class="textonegrita15">País</div>
                    </div>
                </div>
                @for (int index = 0; index < this.Model.Count; index++)
                {
                    var BankAccount = this.Model[index];
                    
                    <div style="float:left; width:700px;">
                        <div style="float:left; width:70px;">
                            <div style="float:left; width:70px; padding-top:15px;" class="textonegrita">
                            @BankAccount.BankName</div>
                        </div>
                        <div style="float:left; width:170px; padding-left:60px;">
                            <div style="float:left; width:170px; padding-top:15px;" class="textonegrita13">
                            @BankAccount.BankAccountNumber</div>
                        </div>
                        <div style="float:left; width:70px; padding-left:40px;">
                            <div style="float:left; width:70px; padding-top:15px;" class="textonegrita">
                            @BankAccount.Password</div>
                        </div>
                        <div style="float:left; width:70px; padding-left:40px;">
                            <div style="float:left; width:70px; padding-top:15px;" class="textonegrita">
                            @BankAccount.Country</div>
                        </div>
                        <div style="float:left; width:70px; padding-left:55px;">
                             <div style="float:left; padding-top:15px;"></div>
                        </div>
                    </div>
                }
        	</div>
        </div>
        <div style="float:left; width:730px;">
            	<div style="float:left; padding-left:530px; width:600px; padding-top:45px;" class="negro11"><a href="/BankAccount/Create">Crear nueva cuenta</a></div>
        	</div>
        <div style="float:left; width:738px; margin-top:50px; height:1px; background:#b5b5b3;"></div>
        <div style="float:left; width:738px; padding-top:4px;">
            <div style="float:right;"><!-- <a href="#"><img src="images/guardar.png" border="0"></a> --></div>
            <div style="float:right; padding-right:5px;"><a href="JavaScript:history.back(-1);"><img src="/Content/images/volver.png" border="0"></a></div>
        </div>
    </div>
  </div>