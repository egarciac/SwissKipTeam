@model IEnumerable<SwissKip.Web.Models.RecipientAddModel>
@{
    ViewBag.Title = "Recipient";
}
<div id="contenido">
    <div style="float: left; padding-left: 100px;">
        <div class="rojo25" style="width:738px;">
            <div style="float: left;">My Account :: @ViewBag.Title</div>
            <div style="float:right; padding-top:10px;" class="negro12">
                @Html.ActionLink("[+] My Account", "Index", "MyAccount")
                &nbsp;&nbsp;&nbsp;
                @Html.ActionLink("[+] Change Password", "ChangePassword", "Owner")
                &nbsp;&nbsp;&nbsp;
                @Html.ActionLink("[+] Witness", "EditWitness", "Owner")
                &nbsp;&nbsp;&nbsp;
                @Html.ActionLink(" [+] Dataheir", "AddRecipient", "Owner")
            </div>
        </div>
        <div style="float: left; width: 738px; height: 1px; z-index: 1; margin-top: -3px; background: #b5b5b3;"></div>
        @using (Html.BeginForm())
        {
            <div id="dataheir" style="float:left; width:738px;">
            @foreach (var dataheir in Model)
            {
                Html.RenderPartial("_Recipient", dataheir);
            }    
            </div>
            <div style="float: left; width: 738px; margin-top: 15px; height: 1px; background: #b5b5b3;">
            </div>
            <div style="float: left; width: 738px; padding-top: 4px;">
                <div style="float: right;">
                    <input type="image" name="Enviar" value="Enviar" src="/Content/images/guardar.png" />
                </div>
                <div style="float: right; padding-right: 5px;">
                    <a href="@Url.Action("Index")"><img src="/Content/images/volver.png" border="0"></a>
                </div>
            </div>
        }
    </div>
</div>