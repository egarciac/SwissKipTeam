@model List<SwissKip.Web.Models.DigitalAccountModel>
@{
    ViewBag.Title = "Index";
}

<div id="contenido">
  	<div style="float:left; padding-left:100px;">
    	<div class="rojo25" style="float:left; z-index:0;">
        	<div style="float:left; padding-bottom:2px;">My stuff</div>
        	<div style="float:left; padding-left:235px;"><a href="/FileManager/ListFiles" onMouseDown="MM_swapImage('menu11','','/Content/images/documentos.png',1)" onMouseOver="MM_swapImage('menu11','','/Content/images/documentos-on.png',0)" onMouseOut="MM_swapImgRestore()"><img src="/Content/images/documentos.png" border="0" name="menu11"></a></div>
            <div style="float:left; padding-left:8px;"><a href="/BankAccount/Index" onMouseDown="MM_swapImage('menu12','','/Content/images/cuentas-bancarias-off.png',1)" onMouseOver="MM_swapImage('menu12','','/Content/images/cuentas-bancarias.png',0)" onMouseOut="MM_swapImgRestore()"><img src="/Content/images/cuentas-bancarias-off.png" border="0" name="menu12"></a></div>
            <div style="float:left; padding-left:8px;"><img src="/Content/images/cuentas-digitales-on.png"></div>
        </div>
    	<div style="float:left; width:738px; height:1px; z-index:1; margin-top:-7px; background:#b5b5b3;"></div>
        <div style="float:left; padding-left:110px; width:830px; padding-top:80px;">
        	<div style="float:left; width:700px;">
                    <div style="float:left; width:70px;">
                        <div style="float:left; width:70px;" class="textonegrita15">WebSite</div>
                    </div>
                    <div style="float:left; width:170px; padding-left:60px;">
                        <div style="float:left; width:170px; padding-left:10px;" class="textonegrita15">User</div>
                    </div>
                    <div style="float:left; width:70px; padding-left:40px;">
                        <div style="float:left; width:70px;" class="textonegrita15">Password</div>
                    </div>
                </div>
                @for (int index = 0; index < this.Model.Count; index++)
                {
                    var DigitalAccount = this.Model[index];
                    
                    <div style="float:left; width:700px;">
                        <div style="float:left; width:70px;">
                            <div style="float:left; width:70px; padding-top:15px;" class="textonegrita">
                            @DigitalAccount.WebSite</div>
                        </div>
                        <div style="float:left; width:170px; padding-left:60px;">
                            <div style="float:left; width:170px; padding-top:15px;" class="textonegrita13">
                            @DigitalAccount.Username</div>
                        </div>
                        <div style="float:left; width:70px; padding-left:40px;">
                            <div style="float:left; width:70px; padding-top:15px;" class="textonegrita">
                            @DigitalAccount.Password</div>
                        </div>
                    </div>
                }
            <div style="float:left; width:730px;">
            	<div style="float:left; padding-left:425px; width:150px; padding-top:12px;" class="negro11"><a href="Create">Crear nueva cuenta</a></div>
        	</div>
        </div>
        <div style="float:left; width:738px; margin-top:60px; height:1px; background:#b5b5b3;"></div>
        <div style="float:left; width:738px; padding-top:4px;">
            <div style="float:right;"><!-- <a href="#"><img src="images/guardar.png" border="0"></a> --></div>
            <div style="float:right; padding-right:5px;"><a href="javascript:history.back(-1);"><img src="/Content/images/volver.png" border="0"></a></div>
        </div>
    </div>
  </div>